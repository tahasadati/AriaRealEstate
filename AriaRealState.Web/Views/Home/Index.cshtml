@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts {
    <script src="~/js/main.js"></script>
    <script>
        const tabButtonsDesktop = document.querySelectorAll(
          "#tab-villa, #tab-land",
        );

        const contentsDesktop = {
          villa: document.getElementById("content-villa"),
          land: document.getElementById("content-land"),
        };

        tabButtonsDesktop.forEach((button) => {
          button.addEventListener("click", () => {
            const tab = button.dataset.tab;

            Object.values(contentsDesktop).forEach((content) => {
              content.classList.add("hidden");
            });

            contentsDesktop[tab].classList.remove("hidden");

            tabButtonsDesktop.forEach((btn) => {
              btn.classList.remove("bg-primary", "text-white");
              btn.classList.add("bg-white", "text-black");
            });

            button.classList.remove("bg-white", "text-black");
            button.classList.add("bg-primary", "text-white");
          });
        });
    </script>
    <script>
        const tabButtonsMobile = document.querySelectorAll(
          "#tab-villa-mobile, #tab-land-mobile",
        );

        const contentsMobile = {
          villa: document.getElementById("content-villa-mobile"),
          land: document.getElementById("content-land-mobile"),
        };

        tabButtonsMobile.forEach((button) => {
          button.addEventListener("click", () => {
            const tab = button.dataset.tab;

            Object.values(contentsMobile).forEach((content) => {
              content.classList.add("hidden");
            });

            contentsMobile[tab].classList.remove("hidden");

            tabButtonsMobile.forEach((btn) => {
              btn.classList.remove("bg-primary", "text-white");
              btn.classList.add("bg-white", "text-black");
            });

            button.classList.remove("bg-white", "text-black");
            button.classList.add("bg-primary", "text-white");
          });
        });
    </script>
    <script>
        const landSwiper = new Swiper(".landSwiper", {
          loop: true,

          grabCursor: true,
          autoplay: {
            delay: 3000,
            disableOnInteraction: false,
            pauseOnMouseEnter: true,
          },

          breakpoints: {
            0: {
              slidesPerView: 1.1,
            },
            640: {
              slidesPerView: 2.2,
            },
            1024: {
              slidesPerView: 3,
            },
            1280: {
              slidesPerView: 4,
            },
          },
        });
    </script>
    <script>
        new Swiper(".urgentSwiper", {
          loop: true,
          grabCursor: true,
          speed: 300,

          slidesPerView: 4,
          slidesPerGroup: 8,

          grid: {
            rows: 2,
            fill: "row",
          },

          autoplay: {
            delay: 3500,
            disableOnInteraction: false,
            pauseOnMouseEnter: true,
          },

          breakpoints: {
            0: {
              slidesPerView: 1.1,
              slidesPerGroup: 2,
              grid: { rows: 1 },
            },
            640: {
              slidesPerView: 2.2,
              slidesPerGroup: 4,
              grid: { rows: 2 },
            },
            1024: {
              slidesPerView: 3,
              slidesPerGroup: 8,
              grid: { rows: 2 },
            },
            1280: {
              slidesPerView: 4,
              slidesPerGroup: 8,
              grid: { rows: 2 },
            },
          },
        });
        const imageSwiper = new Swiper(".imageSwiper", {
          loop: true,

          breakpoints: {
            0: {
              slidesPerView: 1,
            },
            640: {
              slidesPerView: 2,
            },
            1024: {
              slidesPerView: 3,
            },
            1280: {
              slidesPerView: 3,
            },
          },
          autoplay: {
            delay: 3000,
            disableOnInteraction: false,
            pauseOnMouseEnter: true,
          },
        });
    </script>
}
